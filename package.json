{
  "name": "storybook-addon-react-flow-docgen",
  "version": "0.9.0",
  "description": "storybook.js addon generating documentation using react-docgen",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --ignore src/__tests__,src/__mocks__ --out-dir dist -s",
    "lint": "eslint src --ext .js --cache",
    "test": "jest",
    "prepare": "npm run build",
    "example": "start-storybook -p 3000 -c ./example/.storybook -s ./example"
  },
  "author": "Adam Mak≈õwiej <adam.makswiej@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vertexbz/storybook-addon-react-flow-docgen.git"
  },
  "license": "MIT",
  "dependencies": {
    "github-markdown-css": "^2.10.0",
    "marked": "^0.5.0"
  },
  "devDependencies": {
    "@storybook/addons": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-codegen": "^3.0.0",
    "babel-plugin-react-docgen": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-no-only-tests": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.80.0",
    "jest": "^23.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.3.0",
    "react-dom": "^16.5.0",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "@storybook/addons": "^3",
    "babel-plugin-react-docgen": "^2"
  },
  "keywords": [
    "storybook",
    "addon",
    "react",
    "flow",
    "docgen"
